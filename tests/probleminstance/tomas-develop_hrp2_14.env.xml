        <translation>0 0 0</translation>
        <diffuseColor>.0 .0 .6</diffuseColor>
      	<ambientColor>.2 .2 .2</ambientColor>
        <transparency>0.3</transparency>
      </geom>
    </body>
  </kinbody>
