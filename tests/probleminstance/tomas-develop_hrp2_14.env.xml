        </mass>
      </body>
    </kinbody>
    <kinbody>
      <body name="TRANS_Y_LINK">
        <mass type="mimicgeom">
          <total>0</total>
        </mass>
      </body>
    </kinbody>
    <kinbody>
      <body name="TRANS_Z_LINK">
        <mass type="mimicgeom">
          <total>0</total>
        </mass>
      </body>
    </kinbody>
    <kinbody>
      <body name="ROT_R_LINK">
        <mass type="mimicgeom">
