      </body>
    </kinbody>
    <kinbody>
      <body name="ROT_Y_LINK">
        <mass type="mimicgeom">
          <total>0</total>
        </mass>
      </body>
    </kinbody>
    <robot file="hrp2_14.dae" name="HRP2">
      <kinbody>
        <joint name="TRANS_X" type="slider" circular="true">
          <body>TRANS_X_LINK</body>
          <body>TRANS_Y_LINK</body>
          <axis>1 0 0</axis>
          <limits>-10 +10</limits>
        </joint>
        <joint name="TRANS_Y" type="slider" circular="true">
          <body>TRANS_Y_LINK</body>
          <body>TRANS_Z_LINK</body>
          <axis>0 1 0</axis>
          <limits>-10 +10</limits>
        </joint>
        <joint name="TRANS_Z" type="slider" circular="true">
          <body>TRANS_Z_LINK</body>
          <body>ROT_R_LINK</body>
          <axis>0 0 1</axis>
          <limits>-10 +10</limits>
        </joint>
        <joint name="ROT_R" type="hinge" circular="true">
          <body>ROT_R_LINK</body>
          <body>ROT_P_LINK</body>
          <axis>1 0 0</axis>
        </joint>
        <joint name="ROT_P" type="hinge" circular="true">
          <body>ROT_P_LINK</body>
          <body>ROT_Y_LINK</body>
          <axis>0 1 0</axis>
        </joint>
        <joint name="ROT_Y" type="hinge" circular="true">
          <body>ROT_Y_LINK</body>
          <body>BODY</body>
          <axis>0 0 1</axis>
        </joint>
      </kinbody>
    </robot>
  </robot>
  <kinbody name="box">
    <body type="static">
      <translation>0.85 0 0.130</translation>
      <geom type="box">
        <extents>0.27 0.3 0.125</extents>
